   "id": "d3ac2e9a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "  Train RMSE: 0.1647, MAE: 0.1197, R²: 0.9441\n",
      "  Test RMSE: 0.2030, MAE: 0.1425, R²: 0.9170\n"
     ]
    }
   ],
   "source": [
    "print(f\"  Train RMSE: {train_rmse:.4f}, MAE: {train_mae:.4f}, R²: {train_r2:.4f}\")\n",
    "print(f\"  Test RMSE: {test_rmse:.4f}, MAE: {test_mae:.4f}, R²: {test_r2:.4f}\")"
   ]
  },
