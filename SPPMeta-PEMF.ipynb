   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "from sklearn.ensemble import RandomForestRegressor, ExtraTreesRegressor\n",
    "from sklearn.tree import DecisionTreeRegressor\n",
    "from sklearn.kernel_ridge import KernelRidge\n",
    "from sklearn.neighbors import KNeighborsRegressor\n",
    "import xgboost as xgb\n",
    "from xgboost import XGBRegressor\n",
    "\n",
    "from sklearn.model_selection import KFold, GridSearchCV, train_test_split\n",
    "from sklearn.metrics import mean_absolute_error, r2_score, mean_squared_error, make_scorer\n",
    "from sklearn.feature_selection import RFECV\n",
    "\n",
    "from sklearn.preprocessing import MinMaxScaler, StandardScaler\n",
    "\n",
    "from scipy.stats import pearsonr\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "from itertools import product\n",
    "from tqdm import tqdm\n",
    "from time import sleep"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "5dfe21e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rmse(y_true, y_pred):\n",
    "    return mean_squared_error(y_true, y_pred, squared=False)\n",
    "\n",
    "rmse_scorer = make_scorer(rmse, greater_is_better=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "9dd9d623",
   "metadata": {},
   "outputs": [],
   "source": [
    "mae_scorer = make_scorer(mean_absolute_error, greater_is_better=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "2df0a4a4",
   "metadata": {},
   "outputs": [],
   "source": [
    "SHyper = pd.read_csv('SCouping_product_smile.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "70412e01",
   "metadata": {},
   "outputs": [],
   "source": [
    "S_CmordredC = pd.read_csv('CatalystSC.csv')\n",
    "S_CmordredC_fixes = S_CmordredC.dropna(axis=1, how='any')\n",
    "S_CmordredC_fix = S_CmordredC_fixes.copy()\n",
    "S_CmordredC_fix.drop(S_CmordredC_fix.columns[0], axis = 1, inplace = True)\n",
    "S_CmordredC_fix.rename(columns=lambda x: x + \"-1\", inplace=True)\n",
    "\n",
    "S_CmordredI = pd.read_csv('ImineSC.csv')\n",
    "S_CmordredI_fixes = S_CmordredI.dropna(axis=1, how='any')\n",
    "S_CmordredI_fix = S_CmordredI_fixes.copy()\n",
    "S_CmordredI_fix.drop(S_CmordredI_fix.columns[0], axis = 1, inplace = True)\n",
    "S_CmordredI_fix.rename(columns=lambda x: x + \"-2\", inplace=True)\n",
    "\n",
    "S_CmordredT = pd.read_csv('ThiolSC.csv')\n",
    "S_CmordredT_fixes = S_CmordredT.dropna(axis=1, how='any')\n",
    "S_CmordredT_fix = S_CmordredT_fixes.copy()\n",
    "S_CmordredT_fix.drop(S_CmordredT_fix.columns[0], axis = 1, inplace = True)\n",
    "S_CmordredT_fix.rename(columns=lambda x: x + \"-3\", inplace=True)\n",
    "\n",
    "S_CmordredP = pd.read_csv('ProductSC.csv')\n",
    "S_CmordredP_fixes = S_CmordredP.dropna(axis=1, how='any')\n",
    "S_CmordredP_fix = S_CmordredP_fixes.copy()\n",
    "S_CmordredP_fix.drop(S_CmordredP_fix.columns[0], axis = 1, inplace = True)\n",
    "S_CmordredP_fix.rename(columns=lambda x: x + \"-4\", inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "1474c1f4",
   "metadata": {},
   "outputs": [],
   "source": [
    "MordredPP = pd.concat([S_CmordredC_fix, S_CmordredI_fix, S_CmordredT_fix, S_CmordredP_fix], axis = 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "935fb764",
   "metadata": {},
   "outputs": [],
   "source": [
    "from rdkit import Chem\n",
    "from rdkit.Chem import Descriptors\n",
    "from rdkit.ML.Descriptors import MoleculeDescriptors"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "9dbf5e7d",
   "metadata": {},
   "outputs": [],
   "source": [
    "mols1 = [Chem.MolFromSmiles(smi) for smi in SHyper['Catalyst']]\n",
    "mols2 = [Chem.MolFromSmiles(smi) for smi in SHyper['Imine']]\n",
    "mols3 = [Chem.MolFromSmiles(smi) for smi in SHyper['Thiol']]\n",
    "mols4 = [Chem.MolFromSmiles(smi) for smi in SHyper['Product']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "4fa35468",
   "metadata": {},
   "outputs": [],
   "source": [
    "from rdkit.Avalon import pyAvalonTools\n",
    "\n",
    "Avs1 = [pyAvalonTools.GetAvalonFP(mol, nBits=2048) for mol in mols1]\n",
    "df_Avs1 = pd.DataFrame(np.array(Avs1))\n",
    "df_Avs1.columns = [f'1_{col}' for col in df_Avs1.columns]\n",
    "\n",
    "Avs2 = [pyAvalonTools.GetAvalonFP(mol, nBits=2048) for mol in mols2]\n",
    "df_Avs2 = pd.DataFrame(np.array(Avs2))\n",
    "df_Avs2.columns = [f'2_{col}' for col in df_Avs2.columns]\n",
    "\n",
    "Avs3 = [pyAvalonTools.GetAvalonFP(mol, nBits=2048) for mol in mols3]\n",
    "df_Avs3 = pd.DataFrame(np.array(Avs3))\n",
    "df_Avs3.columns = [f'3_{col}' for col in df_Avs3.columns]\n",
    "\n",
    "Avs4 = [pyAvalonTools.GetAvalonFP(mol, nBits=2048) for mol in mols4]\n",
    "df_Avs4 = pd.DataFrame(np.array(Avs4))\n",
    "df_Avs4.columns = [f'4_{col}' for col in df_Avs4.columns]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "c8f31296",
   "metadata": {},
   "outputs": [],
   "source": [
    "Avs_PEMF = pd.concat([df_Avs1, df_Avs2, df_Avs3, df_Avs4], axis = 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "e131344a",
   "metadata": {},
   "outputs": [],
   "source": [
    "SPP_columns = ['SMR_VSA9-1', 'nAromAtom-3']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "01fd3393",
   "metadata": {},
   "outputs": [],
   "source": [
    "SPP = MordredPP[SPP_columns]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "280f6a74",
   "metadata": {},
   "outputs": [],
   "source": [
    "SPP_Avs_PEMF = pd.concat([SPP, Avs_PEMF], axis = 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "fdde9b7b",
   "metadata": {},
   "outputs": [],
   "source": [
    "X_full = SPP_Avs_PEMF\n",
    "y = SHyper.iloc[:,-2].copy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "78807eaf",
   "metadata": {},
   "outputs": [],
   "source": [
    "X_train_full, X_test, y_train_full, y_test = train_test_split(X_full, y, test_size=475, random_state=88)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "31408ffc",
   "metadata": {},
   "outputs": [],
   "source": [
    "X_train_base = X_train_full.iloc[:, 2:]  \n",
    "X_train_meta = X_train_full.iloc[:, :2]  \n",
    "X_test_base = X_test.iloc[:, 2:]\n",
    "X_test_meta = X_test.iloc[:, :2]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "ee7eb31a",
   "metadata": {},
   "outputs": [],
   "source": [
    "base_models = [\n",
    "    ('rf', RandomForestRegressor(n_estimators=1500, max_features = 0.2, random_state=88)),\n",
    "    ('xgb', XGBRegressor(objective=\"reg:squarederror\", n_estimators=20, learning_rate=0.2, random_state=88)),\n",
    "    ('et', ExtraTreesRegressor(n_estimators=600, max_features = 0.2, random_state=88)),\n",
    "    ('dt', DecisionTreeRegressor(max_depth=9, min_samples_split=8, random_state=88)),\n",
    "    ('krr', KernelRidge(kernel='rbf', alpha=0.005, gamma=0.001))\n",
    "]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "08f06c39",
   "metadata": {},
   "outputs": [],
   "source": [
    "kf = KFold(n_splits=5, shuffle=True, random_state=88)\n",
    "meta_features_train = np.zeros((X_train_base.shape[0], len(base_models)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "0b654f45",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Training base model rf\n",
      "Training base model xgb\n",
      "Training base model et\n",
      "Training base model dt\n",
      "Training base model krr\n"
     ]
    }
   ],
   "source": [
    "for i, (name, model) in enumerate(base_models):\n",
    "    print(f\"Training base model {name}\")\n",
    "    for train_idx, val_idx in kf.split(X_train_base):\n",
    "        X_tr, X_val = X_train_base.iloc[train_idx], X_train_base.iloc[val_idx]\n",
    "        y_tr = y_train_full.iloc[train_idx]\n",
    "        model.fit(X_tr, y_tr)\n",
    "        meta_features_train[val_idx, i] = model.predict(X_val)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "2e5c9e76",
   "metadata": {},
   "outputs": [],
   "source": [
    "meta_features_test = np.zeros((X_test_base.shape[0], len(base_models)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "id": "e556130e",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i, (name, model) in enumerate(base_models):\n",
    "    model.fit(X_train_base, y_train_full)
    "    meta_features_test[:, i] = model.predict(X_test_base)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "72b8d549",
   "metadata": {},
   "outputs": [],
   "source": [
    "meta_features_train = np.hstack([meta_features_train, X_train_meta.values])\n",
    "meta_features_test = np.hstack([meta_features_test, X_test_meta.values])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "08c2d3cf",
   "metadata": {},
   "outputs": [],
   "source": [
    "meta_model = XGBRegressor(objective=\"reg:squarederror\", random_state=88)\n",
    "param_grid_meta = {\n",
    "    'n_estimators': [10, 20, 30, 40, 50, 70, 90, 100, 300, 500],\n",
    "    'learning_rate': [0.01, 0.03, 0.05, 0.07, 0.1],\n",
    "    'max_depth': [2, 3, 4, 6],\n",
    "    'subsample': [0.6, 0.8, 1.0],\n",
    "    'colsample_bytree': [0.6, 0.8, 1.0]\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "d842416f",
   "metadata": {},
   "outputs": [],
   "source": [
    "scorer = make_scorer(mean_squared_error, greater_is_better=False, squared=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "18ab9f10",
   "metadata": {},
   "outputs": [],
   "source": [
    "grid_search_meta = GridSearchCV(\n",
    "    estimator=meta_model,\n",
    "    param_grid=param_grid_meta,\n",
    "    scoring=scorer,\n",
    "    cv=3,
    "    n_jobs=-1,\n",
    "    verbose=0\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "c266c6c5",
   "metadata": {},
   "outputs": [],
   "source": [
    "grid_search_meta.fit(meta_features_train, y_train_full)\n",
    "best_meta_model = grid_search_meta.best_estimator_"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "7c8953b7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Best Meta Model Parameters: {'colsample_bytree': 0.6, 'learning_rate': 0.1, 'max_depth': 3, 'n_estimators': 70, 'subsample': 0.6}\n",
      "Best Meta Model RMSE: 0.22809885274495978\n"
     ]
    }
   ],
   "source": [
    "print(\"Best Meta Model Parameters:\", grid_search_meta.best_params_)\n",
    "print(\"Best Meta Model RMSE:\", -grid_search_meta.best_score_)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "9bfa5ba3",
   "metadata": {},
   "outputs": [],
   "source": [
    "y_train_pred = best_meta_model.predict(meta_features_train)\n",
    "train_rmse = np.sqrt(mean_squared_error(y_train_full, y_train_pred))\n",
    "train_mae = mean_absolute_error(y_train_full, y_train_pred)\n",
    "train_r2 = r2_score(y_train_full, y_train_pred)\n",
    "\n",
    "y_test_pred = best_meta_model.predict(meta_features_test)\n",
